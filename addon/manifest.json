
{
  "id": "brand-guardian-ai",
  "name": "Brand Guardian AI",
  "version": "1.0.0",
  "description": "Your intelligent brand compliance assistant.",
  "icons": [
    {
      "width": 24,
      "height": 24,
      "path": "icons/icon-24.png"
    },
    {
      "width": 48,
      "height": 48,
      "path": "icons/icon-48.png"
    }
  ],
  "host": {
    "app": "Adobe Express",
    "minVersion": "1.0"
  },
  "main": "index.html",
  "ui": {
    "type": "panel",
    "label": "Brand Guardian"
  },
  "permissions": {
    "document": {
      "read": true,
      "write": ["addText", "addImage", "updateElement", "replacePageBackground"]
    },
    "currentUser": {
      "read": ["name", "email"]
    },
    "network": {
      "domains": []
    }
  }
}
